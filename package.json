    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "validate.js": "^0.13.1"
